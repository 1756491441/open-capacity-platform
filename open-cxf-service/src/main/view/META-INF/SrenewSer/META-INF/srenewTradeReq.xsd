<?xml version="1.0" encoding="utf-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:srenewTradeReq="http://ws.chinaunicom.cn/SrenewSer/unibssBody/srenewTradeReq" targetNamespace="http://ws.chinaunicom.cn/SrenewSer/unibssBody/srenewTradeReq" elementFormDefault="qualified" attributeFormDefault="unqualified">  
